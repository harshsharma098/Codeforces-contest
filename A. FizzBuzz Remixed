#include<iostream>
#include<vector>
#include<algorithm>  
#include<utility>    
#include<iterator>   
#include<cmath>
using namespace std;
void fizzbuzz(int n){
    int a=(n/15)*3;
    int c=n%15;
    if(c>=0){
        a++;
    }
    if(c>=1){
        a++;
    }
    if(c>=2){
        a++;
    }
    cout<<a<<endl;
}

int main() {
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;
        fizzbuzz(n);
    }
    return 0;
}
